#include <iostream>
using namespace std;
int h, w, u = 0, p = 0, l = 0; //h-высота, w - длина, p - счетчик нулей справой стороны,  l - c левой
int main()
{
	cin >> h >> w; // считывание значений
	//блок считывания символов
	for (int i = 0; i < h - 1; ++i) 
	{
		for (int j = 0; j < w; j++)
		{
			char s;
			cin >> s;
		};
	};
	char s; //переменная под символы последнего уровня
	//блок проверки на точки
	for (int j = 0; j < w / 2; j++) //делим общее кол во символов в уровне, как бы на правую и левую часть
	{
		cin >> s;
		if (s != '.') // считаем нулевые блоки слева
			++l;
	};
	cin >> s;
	for (int j = 0; j < w / 2; j++) //делим общее кол во символов в уровне, как бы на правую и левую часть
	{
		cin >> s;
		if (s != '.') // считаем нулевые блоки справа
			++p;
	};
	if (p < l)			//проверяем где равновесие нарушается
		cout << " -1"; //падает на лево
	if (l < p)
		cout << " 1";// падает направо
	if (p == l)
		cout << "0"; // равновесие

}
